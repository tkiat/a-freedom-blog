{
  "name": "a-freedom-blog",
  "version": "1.0.0",
  "description": "A freedom blog",
  "private": true,
  "scripts": {
    "build": "spago build && spago bundle-app",
    "bundle": "rm -rf dist && parcel build index.html --no-source-maps --public-url ./",
    "clean": "rm -rf .parcel-cache dist serve output node_modules .spago",
    "serve": "parcel serve --no-cache --no-source-maps --port 3000 --dist-dir serve index.html",
    "test": "spago test --no-install"
  },
  "author": "Theerawat Kiatdarakun",
  "license": "MIT",
  "dependencies": {
    "highlight.js": "^11.3.1",
    "marked": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sanitize-html": "^2.6.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0-beta.2",
    "parcel": "^2.0.0-beta.2",
    "sass": "^1.43.4"
  }
}
